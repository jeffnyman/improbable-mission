{
  "$schema": "https://json.schemastore.org/prettierrc",
  "printWidth": 80,
  "useTabs": false,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "trailingComma": "all",
  "bracketSpacing": true,
  "objectWrap": "preserve",
  "arrowParens": "always",
  "proseWrap": "preserve",
  "endOfLine": "lf"
}
